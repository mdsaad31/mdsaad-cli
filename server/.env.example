# MDSAAD Proxy Server Environment Variables
# Copy this file to .env and fill in your actual API keys

# Server Configuration
NODE_ENV=production
PORT=3000

# Required API Keys (get these from the respective services)
OPENROUTER_API_KEY=sk-or-v1-your_openrouter_key_here
GROQ_API_KEY=gsk_your_groq_key_here
DEEPSEEK_API_KEY=your_deepseek_key_here
WEATHERAPI_KEY=your_weatherapi_key_here
GEMINI_API_KEY=your_gemini_key_here

# Render/Railway specific (auto-set by platform)
RENDER_EXTERNAL_URL=https://your-app.onrender.com
RAILWAY_STATIC_URL=https://your-app.up.railway.app

# Optional Configuration
CORS_ORIGIN=*
RATE_LIMIT_WINDOW_MS=3600000
RATE_LIMIT_AI_MAX=50
RATE_LIMIT_WEATHER_MAX=100